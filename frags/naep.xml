<Cube name="NAEP">
    <Annotations>
        <Annotation name="source_name">NAEP Reading Assessment</Annotation>
        <Annotation name="source_link">https://www.nationsreportcard.gov/reading_2017/#?grade=4</Annotation>
        <Annotation name="dataset_name">NAEP Reading Assessment</Annotation>
        <Annotation name="topic">Education</Annotation>
    </Annotations>

    <Table name="naep" schema="naep"/>
     
    <Dimension name="Year">
        <Annotations>
            <Annotation name="dim_type">TIME</Annotation>
        </Annotations>
         <Hierarchy hasAll="true">
            <Level name="Year" column="year" type="Numeric" levelType="TimeYears" uniqueMembers="true" />
        </Hierarchy>
    </Dimension>

    <Dimension foreignKey="geoid" name="Geography">
        <Annotations>
            <Annotation name="dim_type">GEOGRAPHY</Annotation>
        </Annotations>
        <Hierarchy hasAll="true" name="City">
            <Table name="places" schema="shapes2017"/>
            <Level name="City" column="geoid" nameColumn="name" uniqueMembers="true" hideMemberIf="IfBlankName" />
        </Hierarchy>
    </Dimension>

    <Dimension name="Grade">
        <Annotations>
            <Annotation name="dim_type">Grade</Annotation>
        </Annotations>
         <Hierarchy hasAll="true">
            <Level name="Grade" column="grade" type="String" uniqueMembers="true" />
        </Hierarchy>
    </Dimension>
    
    <Measure name="Reading Score Average" column="reading_score_average" aggregator="sum" />
    <Measure name="Reading Score National Average" column="reading_score_national_average" aggregator="sum" />
    <Measure name="Reading Score Michigan Average" column="reading_score_michigan_average" aggregator="sum" />
    <Measure name="Reading Score Race - Black" column="reading_score_race_black" aggregator="sum" />
    <Measure name="Reading Score Race - Hispanic" column="reading_score_race_hispanic" aggregator="sum" />
    <Measure name="Reading Score Gender - Male" column="reading_score_gender_male" aggregator="sum" />
    <Measure name="Reading Score Gender - Female" column="reading_score_gender_female" aggregator="sum" />
    <Measure name="Reading Score NSLP Eligible" column="reading_score_nslp_eligible" aggregator="sum" />
    <Measure name="Reading Score NSLP Ineligible" column="reading_score_nslp_ineligible" aggregator="sum" />
    <Measure name="Reading Score Parent edu - No high school" column="reading_score_edu_no_high_school" aggregator="sum" />
    <Measure name="Reading Score Parent edu - High school" column="reading_score_edu_high_school" aggregator="sum" />
    <Measure name="Reading Score Parent edu - Some college" column="reading_score_edu_some_college" aggregator="sum" />
    <Measure name="Reading Score Parent edu - College" column="reading_score_edu_college" aggregator="sum" />
    <Measure name="Reading Score Parent edu - Unknown" column="reading_score_edu_unknown" aggregator="sum" />
    <Measure name="Reading Score Disability - Yes" column="reading_score_disabilities" aggregator="sum" />
    <Measure name="Reading Score Disability - No" column="reading_score_no_disabilities" aggregator="sum" />
    <Measure name="Reading Score ELL - Yes" column="reading_score_ell" aggregator="sum" />
    <Measure name="Reading Score ELL - No" column="reading_score_not_ell" aggregator="sum" />
    <Measure name="Reading Score Percent Proficient - Detroit" column="reading_score_proficient_percent" aggregator="sum" />
    <Measure name="Reading Score Percent Proficient - National" column="reading_score_national_proficient_percent" aggregator="sum" />
    <Measure name="Reading Score Percent Proficient - Michigan" column="reading_score_michigan_proficient_percent" aggregator="sum" />
</Cube>
