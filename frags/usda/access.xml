<Cube name="USDA - Access">
    <Annotations>
        <Annotation name="source_name">United State Department of Agriculture</Annotation>
        <Annotation name="source_link">https://www.ers.usda.gov/</Annotation>
        <Annotation name="dataset_name">USDA</Annotation>
        <Annotation name="topic">Environmental</Annotation>
    </Annotations>

    <Table name="usda" schema="usda_access"/>

    <Dimension name="Year">
        <Annotations>
            <Annotation name="dim_type">TIME</Annotation>
        </Annotations>
         <Hierarchy hasAll="true">  
            <Level name="Year" column="year" type="Numeric" levelType="TimeYears" uniqueMembers="true" />  
        </Hierarchy> 
    </Dimension>

    <Dimension foreignKey="fips" name="Geography">
        <Annotations>
            <Annotation name="dim_type">GEOGRAPHY</Annotation>
        </Annotations>
        <Hierarchy hasAll="true" name="County">
            <Table name="counties" schema="shapes2017"/>
            <Level name="County" column="geoid" nameColumn="name" uniqueMembers="true" hideMemberIf="IfBlankName" />
        </Hierarchy>
    </Dimension>

    <Dimension name="Age">
        <Annotations>
            <Annotation name="dim_type">AGE</Annotation>
        </Annotations>
    </Dimension>

    <Dimension name="Race">
        <Annotations>
            <Annotation name="dim_type">RACE</Annotation>
        </Annotations>
    </Dimension>

    <!-- Age -->
    <Measure name="Percent Children with low access to store" column="pct_laccess_child" aggregator="avg" />
    <Measure name="Percent Seniors with low access to store" column="pct_laccess_seniors" aggregator="avg" />
    <!-- Race -->
    <Measure name="Percent White with low access to store" column="pct_laccess_white" aggregator="avg" />
    <Measure name="Percent Black with low access to store" column="pct_laccess_black" aggregator="avg" />
    <Measure name="Percent Hispanic ethnicity with low access to store" column="pct_laccess_hisp" aggregator="avg" />
    <Measure name="Percent Asian with low access to store" column="pct_laccess_nhasian" aggregator="avg" />
    <Measure name="Percent American Indian or Alaska Native with low access to store" column="pct_laccess_nhna" aggregator="avg" />
    <Measure name="Percent Hawaiian or Pacific Islander with low access to store" column="pct_laccess_nhpi" aggregator="avg" />
    <Measure name="Percent Multiracial with low access to store" column="pct_laccess_multir" aggregator="avg" />
</Cube>
