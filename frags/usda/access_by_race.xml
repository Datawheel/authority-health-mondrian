<Cube name="USDA - Low-Access by Race">
    <Annotations>
        <Annotation name="source_name">United State Department of Agriculture</Annotation>
        <Annotation name="source_link">https://www.ers.usda.gov/</Annotation>
        <Annotation name="dataset_name">USDA</Annotation>
        <Annotation name="topic">Environmental</Annotation>
    </Annotations>

    <Table name="usda_access_by_race" schema="usda"/>

    <Dimension name="Year">
        <Annotations>
            <Annotation name="dim_type">TIME</Annotation>
        </Annotations>
         <Hierarchy hasAll="true">
            <Level name="Year" column="year" type="Numeric" levelType="TimeYears" uniqueMembers="true" />
        </Hierarchy>
    </Dimension>

    <Dimension foreignKey="fips" name="Geography">
        <Annotations>
            <Annotation name="dim_type">GEOGRAPHY</Annotation>
        </Annotations>
        <Hierarchy hasAll="true" name="County">
            <Table name="counties" schema="shapes2017"/>
            <Level name="County" column="geoid" nameColumn="namelsad" uniqueMembers="true" hideMemberIf="IfBlankName" />
        </Hierarchy>
    </Dimension>

    <Dimension name="Race">
        <Annotations>
            <Annotation name="dim_type">RACE</Annotation>
        </Annotations>
        <Hierarchy hasAll="true">
            <Level name="Race Group" column="race_group" uniqueMembers="true" />
        </Hierarchy>
    </Dimension>

    <Measure name="Percent" column="percent" aggregator="avg" />
</Cube>
