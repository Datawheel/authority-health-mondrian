    <Measure aggregator="None" dataType="Numeric" name="moe_t" visible="false">
      <MeasureExpression>
        <SQL dialect="monetdb">sqrt(sum(power(moe_t / 1.645, 2))) * 1.645</SQL>
      </MeasureExpression>
    </Measure>
