<Cube name="NAEP - Reading Scores">
    <Annotations>
        <Annotation name="source_name">NAEP</Annotation>
        <Annotation name="source_description">NAEP Reading Assessment</Annotation>
        <Annotation name="dataset_link">https://www.nationsreportcard.gov/reading_2017/#?grade=4</Annotation>
        <Annotation name="dataset_name">NAEP Reading Assessment</Annotation>
        <Annotation name="topic">Education</Annotation>
        <Annotation name="subtopic">Reading Assessment</Annotation>
        <Annotation name="details"></Annotation>
    </Annotations>

    <Table name="reading_scores" schema="naep"/>

    <Dimension name="Year">
        <Annotations>
            <Annotation name="dim_type">TIME</Annotation>
        </Annotations>
         <Hierarchy hasAll="true">
            <Level name="Year" column="year" type="Numeric" levelType="TimeYears" uniqueMembers="true" />
        </Hierarchy>
    </Dimension>

    <Dimension foreignKey="geoid" name="Geography">
        <Annotations>
            <Annotation name="dim_type">GEOGRAPHY</Annotation>
        </Annotations>
        <Hierarchy hasAll="true" name="Nation">
            <Table name="us" schema="shapes2017"/>
            <Level name="Nation" column="geoid" nameColumn="name" uniqueMembers="true" hideMemberIf="IfBlankName" />
        </Hierarchy>
        <Hierarchy hasAll="true" name="State">
            <Table name="states" schema="shapes2017"/>
            <Level name="State" column="geoid" nameColumn="name" uniqueMembers="true" hideMemberIf="IfBlankName" />
        </Hierarchy>Place
        <Hierarchy hasAll="true" name="Place">
            <Table name="places" schema="shapes2017"/>
            <Level name="Place" column="geoid" nameColumn="name" uniqueMembers="true" hideMemberIf="IfBlankName" />
        </Hierarchy>
    </Dimension>

    <Dimension name="Grade">
        <Annotations>
            <Annotation name="dim_type">Grade</Annotation>
        </Annotations>
         <Hierarchy hasAll="true">
            <Level name="Grade" column="grade" type="String" uniqueMembers="true" />
        </Hierarchy>
    </Dimension>

    <Measure name="Average Reading Score" column="average_score" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Score</Annotation>
        </Annotations>
    </Measure>
    <Measure name="Reading Proficiency" column="proficiency" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Percentage</Annotation>
        </Annotations>
    </Measure>
</Cube>
