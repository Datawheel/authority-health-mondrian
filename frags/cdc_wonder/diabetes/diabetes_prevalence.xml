<Cube name="CDC Wonder - Diabetes Prevalence by Sex">
    <Annotations>
        <Annotation name="source_name">CDC Wonder</Annotation>
        <Annotation name="source_description">CDC Wonder</Annotation>
        <Annotation name="dataset_link">https://www.cdc.gov/diabetes/data/countydata/countydataindicators.html</Annotation>
        <Annotation name="dataset_name">CDC Wonder</Annotation>
        <Annotation name="topic">Health Conditions</Annotation>
        <Annotation name="subtopic">Obesity and Diabetes</Annotation>
        <Annotation name="details">Diabetes diagnosis spans 2004-2013 at the county level and can be broken down by sex.</Annotation>
    </Annotations>

    <Table name="diabetes_prevalence_by_sex" schema="cdc_wonder"/>

    <Dimension name="Year">
        <Annotations>
            <Annotation name="dim_type">TIME</Annotation>
        </Annotations>
         <Hierarchy hasAll="true">
            <Level name="Year" column="year" type="Numeric" levelType="TimeYears" uniqueMembers="true" />
        </Hierarchy>
    </Dimension>

    <Dimension foreignKey="county_fips" name="Geography">
        <Annotations>
            <Annotation name="dim_type">GEOGRAPHY</Annotation>
        </Annotations>
        <Hierarchy hasAll="true" name="County">
            <Table name="counties" schema="shapes2017"/>
            <Level column="state_id" name="State" nameColumn="state_name" uniqueMembers="true"/>
            <Level column="geoid" name="County" nameColumn="namelsad" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <Dimension name="Sex">
        <Annotations>
            <Annotation name="dim_type">SEX</Annotation>
        </Annotations>
        <Hierarchy hasAll="true">
            <Level name="Sex" column="sex" uniqueMembers="true"/>
        </Hierarchy>
    </Dimension>

    <Measure name="Diabetes Prevalence Number" column="number" aggregator="sum">
        <Annotations>
            <Annotation name="units_of_measurement">People</Annotation>
            <Annotation name="hide_in_ui">true</Annotation>
        </Annotations>
    </Measure>
    <Measure name="Diabetes Prevalence Percent" column="percent" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Percentage</Annotation>
            <Annotation name="hide_in_ui">true</Annotation>
        </Annotations>
    </Measure>
    <Measure name="Diabetes Prevalence Lower Confidence Limit" column="lower_confidence_limit" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Percentage</Annotation>
            <Annotation name="hide_in_ui">true</Annotation>
        </Annotations>
    </Measure>
    <Measure name="Diabetes Prevalence Upper Confidence Limit" column="upper_confidence_limit" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Percentage</Annotation>
            <Annotation name="hide_in_ui">true</Annotation>
        </Annotations>
    </Measure>

    <Measure name="Age-Adjusted Diabetes Prevalence" caption="Age-Adjusted Diabetes Prevalence (per 100,000)" column="adj_percent" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Percentage</Annotation>
        </Annotations>
    </Measure>
    <Measure name="Age-Adjusted Diabetes Prevalence Lower Confidence Limit" column="adj_lower_confidence_limit" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Percentage</Annotation>
            <Annotation name="error_type">LCI</Annotation>
            <Annotation name="error_for_measure">Age-Adjusted Diabetes Prevalence</Annotation>
        </Annotations>
    </Measure>
    <Measure name="Age-Adjusted Diabetes Prevalence Upper Confidence Limit" column="adj_upper_confidence_limit" aggregator="avg">
        <Annotations>
            <Annotation name="units_of_measurement">Percentage</Annotation>
            <Annotation name="error_type">UCI</Annotation>
            <Annotation name="error_for_measure">Age-Adjusted Diabetes Prevalence</Annotation>
        </Annotations>
    </Measure>
</Cube>
